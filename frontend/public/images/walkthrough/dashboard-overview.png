<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="250" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="250" fill="#f8fafc"/>
  <rect x="20" y="20" width="360" height="40" fill="#3b82f6" rx="4"/>
  <text x="200" y="45" text-anchor="middle" fill="white" font-family="Arial" font-size="16" font-weight="bold">Dashboard Overview</text>
  
  <!-- KPI Cards -->
  <rect x="20" y="80" width="80" height="60" fill="#e0f2fe" rx="4" stroke="#0284c7"/>
  <text x="60" y="100" text-anchor="middle" fill="#0284c7" font-family="Arial" font-size="10">Total Spend</text>
  <text x="60" y="115" text-anchor="middle" fill="#0284c7" font-family="Arial" font-size="14" font-weight="bold">$2.4M</text>
  
  <rect x="110" y="80" width="80" height="60" fill="#f0fdf4" rx="4" stroke="#16a34a"/>
  <text x="150" y="100" text-anchor="middle" fill="#16a34a" font-family="Arial" font-size="10">Active Promos</text>
  <text x="150" y="115" text-anchor="middle" fill="#16a34a" font-family="Arial" font-size="14" font-weight="bold">24</text>
  
  <rect x="200" y="80" width="80" height="60" fill="#fef3c7" rx="4" stroke="#d97706"/>
  <text x="240" y="100" text-anchor="middle" fill="#d97706" font-family="Arial" font-size="10">ROI</text>
  <text x="240" y="115" text-anchor="middle" fill="#d97706" font-family="Arial" font-size="14" font-weight="bold">3.2x</text>
  
  <rect x="290" y="80" width="80" height="60" fill="#fce7f3" rx="4" stroke="#e11d48"/>
  <text x="330" y="100" text-anchor="middle" fill="#e11d48" font-family="Arial" font-size="10">Budget Used</text>
  <text x="330" y="115" text-anchor="middle" fill="#e11d48" font-family="Arial" font-size="14" font-weight="bold">68%</text>
  
  <!-- Chart Area -->
  <rect x="20" y="160" width="180" height="70" fill="#ffffff" rx="4" stroke="#e5e7eb"/>
  <text x="110" y="180" text-anchor="middle" fill="#6b7280" font-family="Arial" font-size="12">Performance Chart</text>
  <polyline points="30,220 50,200 70,210 90,190 110,195 130,185 150,180 170,175 190,170" 
            fill="none" stroke="#3b82f6" stroke-width="2"/>
  
  <!-- Activity Feed -->
  <rect x="210" y="160" width="160" height="70" fill="#ffffff" rx="4" stroke="#e5e7eb"/>
  <text x="290" y="180" text-anchor="middle" fill="#6b7280" font-family="Arial" font-size="12">Recent Activity</text>
  <circle cx="225" cy="195" r="3" fill="#10b981"/>
  <text x="235" y="199" fill="#374151" font-family="Arial" font-size="10">New promotion created</text>
  <circle cx="225" cy="210" r="3" fill="#f59e0b"/>
  <text x="235" y="214" fill="#374151" font-family="Arial" font-size="10">Budget updated</text>
</svg>