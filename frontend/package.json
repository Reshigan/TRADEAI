    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.127",
    "@mui/material": "^5.12.1",
    "@mui/x-data-grid": "^6.2.0",
    "@mui/x-date-pickers": "^6.2.0",
    "@mui/x-tree-view": "^8.14.0",
    "@reduxjs/toolkit": "^1.9.5",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.3.6",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.2.1",
    "date-fns": "^2.29.3",
    "fill-range": "^7.1.1",
    "formik": "^2.2.9",
    "hoopy": "^0.1.4",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.553.0",
