{
  "name": "tradeai-platform",
  "version": "1.0.0",
  "description": "Multi-tenant trading platform with AI-powered analytics",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "build": "docker-compose build",
    "dev": "docker-compose -f docker-compose.dev.yml up",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "npm run test:unit:backend && npm run test:unit:frontend",
    "test:unit:backend": "cd backend && npm test",
    "test:unit:frontend": "cd frontend && npm test",
    "test:integration": "cd backend && npm run test:integration",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:full": "./run-e2e-tests.sh",
    "test:e2e:production": "PROD_URL=https://tradeai.gonxt.tech npx playwright test tests/e2e/production-test.spec.js",
    "test:e2e:production:headed": "PROD_URL=https://tradeai.gonxt.tech npx playwright test tests/e2e/production-test.spec.js --headed",
    "test:e2e:production:report": "npx playwright show-report",
    "test:api": "cd backend && npm run test:api",
    "test:security": "npm audit && npm run test:security:backend && npm run test:security:frontend",
    "test:security:backend": "cd backend && npm audit && snyk test",
    "test:security:frontend": "cd frontend && npm audit && snyk test",
    "test:migrations": "cd backend && npm run test:migrations",
    "test:pwa": "cd frontend && npm run test:pwa",
    "test:go-live": "node scripts/runGoLiveTests.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:coverage:watch": "jest --config jest.config.js --coverage --watch",
    "test:coverage:check": "nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "test:unit:jest": "jest --config jest.config.js --testPathPattern=\"__tests__\"",
    "test:integration:jest": "jest --config jest.config.js --testPathPattern=\"tests/integration\"",
    "test:e2e:cypress": "cypress run",
    "test:e2e:cypress:open": "cypress open",
    "test:all:comprehensive": "./scripts/run-all-tests.sh",
    "test:performance": "artillery run tests/performance/load-test.yml",
    "test:accessibility": "lighthouse http://localhost:3000 --only-categories=accessibility",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && npm run format",
    "format:frontend": "cd frontend && npm run format",
    "deploy:staging": "npm run test:go-live && docker-compose -f docker-compose.staging.yml up -d",
    "deploy:production": "npm run test:go-live && ./scripts/deploy-production.sh",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "logs": "docker-compose logs -f",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "health": "curl -f http://localhost:3000/health && curl -f http://localhost:3001/health.json",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install",
    "clean": "docker-compose down -v && docker system prune -f",
    "docs": "npm run docs:backend && npm run docs:frontend",
    "docs:backend": "cd backend && npm run docs",
    "docs:frontend": "cd frontend && npm run docs"
  },
  "keywords": [
    "trading",
    "multi-tenant",
    "analytics",
    "ai",
    "react",
    "nodejs",
    "postgresql",
    "redis",
    "docker"
  ],
  "author": "TRADEAI Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@cypress/code-coverage": "^3.12.0",
    "@faker-js/faker": "^8.2.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "artillery": "^2.0.21",
    "artillery-plugin-expect": "^2.20.0",
    "audit-ci": "^6.6.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.0",
    "cypress": "^13.3.0",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-transform-css": "^6.0.1",
    "jest-transform-file": "^1.1.1",
    "jsdoc": "^4.0.2",
    "lighthouse": "^11.1.0",
    "lint-staged": "^14.0.0",
    "mongodb-memory-server": "^8.15.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "snyk": "^1.1200.0",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Reshigan/TRADEAI.git"
  },
  "bugs": {
    "url": "https://github.com/Reshigan/TRADEAI/issues"
  },
  "homepage": "https://github.com/Reshigan/TRADEAI#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
